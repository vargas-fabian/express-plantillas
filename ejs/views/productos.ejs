<h1>Vista de productos</h1>
<% if(productos.length){ %>
    <h1>Lista de productos: </h1>
    <ul>
        <% productos.forEach(function(element){ %>
            <li><%= element.producto.title %></li>
        <% }); %>
    </ul>
<% } else{ %>
    <h3>No se encontraron productos</h3>
 <% } %>
<p>Puedes cargar productos:</p>
<a href='/'>Cargar</a>
